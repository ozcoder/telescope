---
const year = new Date().getFullYear();
---

<!doctype html>
<!-- saved from url=(0025)https://telescopetest.io/ -->
<html lang="en">
  <head
    ><meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
      Cloudflare Telescope - Cross-Browser Performance Testing Agent
    </title>
    <meta
      name="description"
      content="Diagnostic performance testing across Chrome, Firefox, Safari, and Edge. Built by Cloudflare."
    />
    <link rel="icon" type="image/png" href="/favicon.png" />
    <style>
      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
      }

      :root {
        --deep-purple: #1a0b2e;
        --purple: #2d1b4e;
        --magenta: #8b3a8c;
        --pink: #d84a8c;
        --orange: #ff6b35;
        --gold: #f7b731;
        --text-light: #e0e0e0;
        --text-white: #ffffff;
      }

      body {
        font-family:
          -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu,
          Cantarell, sans-serif;
        background: var(--deep-purple);
        color: var(--text-light);
        line-height: 1.6;
        overflow-x: hidden;
        cursor: default;
      }

      /* Custom cursor for interactive elements */
      a,
      button,
      .interactive {
        cursor: pointer;
      }

      /* Starfield Background with mouse interaction */
      .stars {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        pointer-events: none;
        z-index: 0;
      }

      .star {
        position: absolute;
        width: 2px;
        height: 2px;
        background: white;
        border-radius: 50%;
        animation: twinkle 3s infinite;
        transition: transform 0.3s ease;
      }

      @keyframes twinkle {
        0%,
        100% {
          opacity: 0.2;
        }
        50% {
          opacity: 0.9;
        }
      }

      /* Shooting stars - occasional effect */
      @keyframes shootingStar {
        0% {
          transform: translateX(0) translateY(0) rotate(-45deg);
          opacity: 1;
        }
        100% {
          transform: translateX(300px) translateY(300px) rotate(-45deg);
          opacity: 0;
        }
      }

      .shooting-star {
        position: fixed;
        width: 2px;
        height: 80px;
        background: linear-gradient(
          to bottom,
          rgba(255, 255, 255, 1),
          transparent
        );
        animation: shootingStar 1.5s ease-out;
        pointer-events: none;
        z-index: 1;
      }

      /* Hero Section */
      .hero {
        position: relative;
        min-height: 100vh;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        text-align: center;
        padding: 100px 20px 80px;
        overflow: hidden;
      }

      /* Observatory Background */
      .hero::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-image: url('data:image/webp;base64,');
        background-size: contain;
        background-position: center center;
        background-repeat: no-repeat;
        opacity: 0.9;
        z-index: 1;
        transition: transform 0.3s ease;
      }

      /* Parallax effect on scroll */
      .hero.scrolled::before {
        transform: translateY(-20px);
      }

      /* Radial gradient overlay */
      .hero::after {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: radial-gradient(
          ellipse at center,
          rgba(26, 11, 46, 0.3) 0%,
          rgba(26, 11, 46, 0.6) 50%,
          rgba(26, 11, 46, 0.85) 100%
        );
        z-index: 2;
      }

      .hero-content {
        position: relative;
        z-index: 3;
        max-width: 1000px;
      }

      /* Logo with interactive glow */
      .logo {
        font-size: 5rem;
        font-weight: 900;
        color: #ffffff;
        margin-bottom: 30px;
        letter-spacing: 3px;
        line-height: 1.1;
        text-transform: uppercase;
        text-shadow:
          0 0 40px rgba(255, 255, 255, 0.4),
          0 0 80px rgba(255, 107, 53, 0.3),
          0 4px 8px rgba(0, 0, 0, 0.9),
          0 8px 16px rgba(0, 0, 0, 0.7),
          0 12px 24px rgba(0, 0, 0, 0.5);
        animation: textPulse 6s ease-in-out infinite;
        transition: transform 0.3s ease;
      }

      .logo:hover {
        transform: scale(1.02);
      }

      .logo-cloudflare {
        display: block;
        font-size: 2.5rem;
        letter-spacing: 8px;
        font-weight: 700;
        color: #ff6b35;
        text-shadow:
          0 0 30px rgba(255, 107, 53, 0.6),
          0 4px 8px rgba(0, 0, 0, 0.9),
          0 8px 16px rgba(0, 0, 0, 0.7);
        transition: letter-spacing 0.3s ease;
      }

      .logo:hover .logo-cloudflare {
        letter-spacing: 10px;
      }

      .logo-telescope {
        display: block;
        font-size: 5.5rem;
        letter-spacing: 12px;
        margin-top: 10px;
        transition: letter-spacing 0.3s ease;
      }

      .logo:hover .logo-telescope {
        letter-spacing: 16px;
      }

      @keyframes textPulse {
        0%,
        100% {
          text-shadow:
            0 0 40px rgba(255, 255, 255, 0.4),
            0 0 80px rgba(255, 107, 53, 0.3),
            0 4px 8px rgba(0, 0, 0, 0.9),
            0 8px 16px rgba(0, 0, 0, 0.7),
            0 12px 24px rgba(0, 0, 0, 0.5);
        }
        50% {
          text-shadow:
            0 0 60px rgba(255, 255, 255, 0.6),
            0 0 120px rgba(216, 74, 140, 0.5),
            0 4px 8px rgba(0, 0, 0, 0.9),
            0 8px 16px rgba(0, 0, 0, 0.7),
            0 12px 24px rgba(0, 0, 0, 0.5);
        }
      }

      .tagline {
        font-size: 2rem;
        color: #ffffff;
        margin-bottom: 25px;
        font-weight: 500;
        letter-spacing: 1px;
        line-height: 1.5;
        text-shadow:
          0 4px 8px rgba(0, 0, 0, 1),
          0 8px 16px rgba(0, 0, 0, 0.8);
        opacity: 0;
        animation: fadeInUp 1s ease forwards 0.3s;
      }

      .subtitle {
        font-size: 1.3rem;
        color: var(--gold);
        margin-bottom: 50px;
        font-weight: 400;
        max-width: 800px;
        margin-left: auto;
        margin-right: auto;
        line-height: 1.8;
        text-shadow:
          0 3px 6px rgba(0, 0, 0, 1),
          0 6px 12px rgba(0, 0, 0, 0.8);
        opacity: 0;
        animation: fadeInUp 1s ease forwards 0.6s;
      }

      @keyframes fadeInUp {
        from {
          opacity: 0;
          transform: translateY(30px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }

      .hero-buttons {
        display: flex;
        gap: 25px;
        justify-content: center;
        flex-wrap: wrap;
        margin-top: 40px;
        opacity: 0;
        animation: fadeInUp 1s ease forwards 0.9s;
      }

      /* Interactive Buttons with Enhanced Effects */
      .btn {
        display: inline-flex;
        align-items: center;
        gap: 12px;
        padding: 22px 50px;
        font-size: 1.2rem;
        text-decoration: none;
        border-radius: 50px;
        font-weight: 700;
        transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
        position: relative;
        overflow: hidden;
        text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
      }

      /* Ripple effect */
      .btn::before {
        content: '';
        position: absolute;
        top: 50%;
        left: 50%;
        width: 0;
        height: 0;
        border-radius: 50%;
        background: rgba(255, 255, 255, 0.3);
        transform: translate(-50%, -50%);
        transition:
          width 0.6s,
          height 0.6s;
      }

      .btn:hover::before {
        width: 400px;
        height: 400px;
      }

      /* Sparkle effect on hover */
      .btn::after {
        content: '✨';
        position: absolute;
        top: -10px;
        right: -10px;
        font-size: 1.5rem;
        opacity: 0;
        transform: scale(0) rotate(0deg);
        transition: all 0.4s ease;
      }

      .btn:hover::after {
        opacity: 1;
        transform: scale(1) rotate(180deg);
      }

      .btn-primary {
        background: linear-gradient(135deg, #d84a8c, #ff6b35);
        color: white;
        box-shadow: 0 15px 40px rgba(216, 74, 140, 0.6);
      }

      .btn-primary:hover {
        transform: translateY(-6px) scale(1.05);
        box-shadow: 0 20px 60px rgba(216, 74, 140, 0.8);
      }

      .btn-primary:active {
        transform: translateY(-3px) scale(1.02);
      }

      .btn-secondary {
        background: rgba(26, 11, 46, 0.95);
        border: 3px solid var(--gold);
        color: var(--gold);
        backdrop-filter: blur(20px);
        box-shadow: 0 10px 40px rgba(0, 0, 0, 0.6);
      }

      .btn-secondary:hover {
        background: rgba(45, 27, 78, 0.98);
        transform: translateY(-6px) scale(1.05);
        box-shadow: 0 20px 50px rgba(247, 183, 49, 0.5);
        border-color: var(--orange);
      }

      .btn-secondary:active {
        transform: translateY(-3px) scale(1.02);
      }

      /* Animated SVG icons in buttons */
      .btn svg {
        transition: transform 0.3s ease;
      }

      .btn:hover svg {
        transform: scale(1.2) rotate(10deg);
      }

      /* Scroll indicator with pulse */
      .scroll-indicator {
        position: absolute;
        bottom: 40px;
        left: 50%;
        transform: translateX(-50%);
        animation: bounce 2.5s infinite;
        z-index: 10;
        cursor: pointer;
        transition: transform 0.3s ease;
      }

      .scroll-indicator:hover {
        animation: none;
        transform: translateX(-50%) scale(1.2);
      }

      @keyframes bounce {
        0%,
        20%,
        50%,
        80%,
        100% {
          transform: translateX(-50%) translateY(0);
        }
        40% {
          transform: translateX(-50%) translateY(-15px);
        }
        60% {
          transform: translateX(-50%) translateY(-8px);
        }
      }

      .scroll-indicator svg {
        width: 40px;
        height: 40px;
        color: var(--gold);
        filter: drop-shadow(0 4px 12px rgba(247, 183, 49, 0.8));
      }

      /* Container */
      .container {
        max-width: 1200px;
        margin: 0 auto;
        padding: 0 20px;
        position: relative;
        z-index: 1;
      }

      /* Sections with entrance animations */
      section {
        margin: 100px 0;
        padding: 70px 50px;
        background: rgba(45, 27, 78, 0.6);
        border-radius: 25px;
        backdrop-filter: blur(15px);
        border: 1px solid rgba(139, 58, 140, 0.3);
        box-shadow: 0 25px 70px rgba(0, 0, 0, 0.4);
        position: relative;
        opacity: 0;
        transform: translateY(50px);
        transition: all 0.6s ease;
      }

      section.visible {
        opacity: 1;
        transform: translateY(0);
      }

      section::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        height: 4px;
        background: linear-gradient(
          90deg,
          var(--pink),
          var(--orange),
          var(--gold)
        );
        border-radius: 25px 25px 0 0;
        animation: shimmer 3s ease-in-out infinite;
      }

      @keyframes shimmer {
        0%,
        100% {
          opacity: 0.8;
        }
        50% {
          opacity: 1;
        }
      }

      /* Floating emoji decorations */
      section::after {
        content: attr(data-emoji);
        position: absolute;
        top: -30px;
        right: 40px;
        font-size: 3rem;
        opacity: 0.2;
        animation: float 4s ease-in-out infinite;
        pointer-events: none;
      }

      @keyframes float {
        0%,
        100% {
          transform: translateY(0px) rotate(0deg);
        }
        50% {
          transform: translateY(-20px) rotate(10deg);
        }
      }

      h2 {
        font-size: 3rem;
        margin-bottom: 35px;
        background: linear-gradient(135deg, var(--orange), var(--gold));
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
        font-weight: 800;
        letter-spacing: 1px;
        position: relative;
        display: inline-block;
      }

      /* Underline animation on hover */
      h2::after {
        content: '';
        position: absolute;
        bottom: -10px;
        left: 0;
        width: 0;
        height: 4px;
        background: linear-gradient(90deg, var(--pink), var(--orange));
        transition: width 0.4s ease;
        border-radius: 2px;
      }

      h2:hover::after {
        width: 100%;
      }

      h3 {
        font-size: 2rem;
        margin: 40px 0 25px;
        color: var(--pink);
        font-weight: 700;
        transition: color 0.3s ease;
      }

      h3:hover {
        color: var(--orange);
      }

      /* Code Blocks with copy button */
      .code-block {
        background: rgba(26, 11, 46, 0.95);
        border: 2px solid var(--magenta);
        border-radius: 15px;
        padding: 30px;
        margin: 25px 0;
        overflow-x: auto;
        position: relative;
        box-shadow: inset 0 2px 10px rgba(0, 0, 0, 0.6);
        transition: all 0.3s ease;
      }

      .code-block:hover {
        border-color: var(--orange);
        box-shadow:
          0 10px 30px rgba(255, 107, 53, 0.3),
          inset 0 2px 10px rgba(0, 0, 0, 0.6);
      }

      .code-block::before {
        content: '⚡';
        position: absolute;
        top: 15px;
        right: 20px;
        font-size: 2rem;
        opacity: 0.3;
        animation: pulse 2s ease-in-out infinite;
      }

      @keyframes pulse {
        0%,
        100% {
          opacity: 0.3;
          transform: scale(1);
        }
        50% {
          opacity: 0.6;
          transform: scale(1.1);
        }
      }

      /* Copy button for code blocks */
      .copy-button {
        position: absolute;
        top: 15px;
        right: 60px;
        background: var(--pink);
        color: white;
        border: none;
        padding: 8px 16px;
        border-radius: 8px;
        font-size: 0.85rem;
        cursor: pointer;
        opacity: 0;
        transition: all 0.3s ease;
        z-index: 10;
      }

      .code-block:hover .copy-button {
        opacity: 1;
      }

      .copy-button:hover {
        background: var(--orange);
        transform: scale(1.05);
      }

      .copy-button:active {
        transform: scale(0.95);
      }

      .copy-button.copied {
        background: #4caf50;
      }

      pre {
        margin: 0;
        font-family: 'Monaco', 'Courier New', monospace;
        color: var(--gold);
        font-size: 1rem;
        line-height: 1.9;
      }

      /* Parameters Grid with stagger animation */
      .params-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));
        gap: 25px;
        margin: 40px 0;
      }

      .param-card {
        background: rgba(26, 11, 46, 0.75);
        padding: 30px;
        border-radius: 15px;
        border-left: 5px solid var(--pink);
        transition: all 0.4s ease;
        position: relative;
        overflow: hidden;
        opacity: 0;
        transform: translateX(-30px);
      }

      .param-card.visible {
        opacity: 1;
        transform: translateX(0);
      }

      /* Sliding gradient effect */
      .param-card::before {
        content: '';
        position: absolute;
        top: 0;
        left: -100%;
        width: 100%;
        height: 100%;
        background: linear-gradient(
          90deg,
          transparent,
          rgba(216, 74, 140, 0.15),
          transparent
        );
        transition: left 0.6s;
      }

      .param-card:hover::before {
        left: 100%;
      }

      .param-card:hover {
        transform: translateX(8px) scale(1.02);
        border-left-color: var(--orange);
        box-shadow: 0 15px 40px rgba(255, 107, 53, 0.3);
      }

      /* Glowing dot indicator */
      .param-card::after {
        content: '';
        position: absolute;
        top: 20px;
        right: 20px;
        width: 8px;
        height: 8px;
        background: var(--gold);
        border-radius: 50%;
        box-shadow: 0 0 10px var(--gold);
        opacity: 0;
        transition: opacity 0.3s ease;
      }

      .param-card:hover::after {
        opacity: 1;
        animation: ping 1.5s ease-in-out infinite;
      }

      @keyframes ping {
        0%,
        100% {
          transform: scale(1);
          opacity: 1;
        }
        50% {
          transform: scale(1.5);
          opacity: 0.5;
        }
      }

      .param-name {
        font-weight: 700;
        color: var(--gold);
        font-size: 1.15rem;
        margin-bottom: 12px;
        transition: transform 0.3s ease;
      }

      .param-card:hover .param-name {
        transform: translateX(5px);
      }

      .param-desc {
        color: var(--text-light);
        font-size: 1rem;
        line-height: 1.7;
      }

      /* Metrics List with 3D effect */
      .metrics-list {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
        gap: 30px;
        margin: 40px 0;
        perspective: 1000px;
      }

      .metric-item {
        background: linear-gradient(
          135deg,
          rgba(139, 58, 140, 0.3),
          rgba(216, 74, 140, 0.2)
        );
        padding: 40px;
        border-radius: 18px;
        border: 2px solid rgba(216, 74, 140, 0.4);
        transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
        text-align: center;
        transform-style: preserve-3d;
        opacity: 0;
        transform: translateY(30px);
      }

      .metric-item.visible {
        opacity: 1;
        transform: translateY(0);
      }

      .metric-item:hover {
        transform: translateY(-10px) rotateX(5deg) rotateY(5deg) scale(1.05);
        box-shadow: 0 25px 60px rgba(216, 74, 140, 0.5);
        border-color: var(--orange);
      }

      .metric-icon {
        font-size: 3.5rem;
        margin-bottom: 20px;
        filter: drop-shadow(0 5px 10px rgba(0, 0, 0, 0.4));
        display: inline-block;
        transition: transform 0.3s ease;
      }

      .metric-item:hover .metric-icon {
        transform: scale(1.2) rotate(10deg);
        animation: wiggle 0.5s ease;
      }

      @keyframes wiggle {
        0%,
        100% {
          transform: scale(1.2) rotate(10deg);
        }
        25% {
          transform: scale(1.2) rotate(5deg);
        }
        75% {
          transform: scale(1.2) rotate(15deg);
        }
      }

      .metric-title {
        font-weight: 700;
        color: var(--gold);
        margin-bottom: 15px;
        font-size: 1.3rem;
      }

      .metric-description {
        color: var(--text-light);
        font-size: 0.95rem;
        line-height: 1.6;
      }

      /* Feature Grid with hover reveal */
      .feature-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
        gap: 35px;
        margin: 50px 0;
      }

      .feature-card {
        background: linear-gradient(
          135deg,
          rgba(45, 27, 78, 0.8),
          rgba(26, 11, 46, 0.8)
        );
        padding: 45px;
        border-radius: 20px;
        border: 2px solid rgba(216, 74, 140, 0.3);
        text-align: center;
        transition: all 0.4s ease;
        position: relative;
        overflow: hidden;
        cursor: pointer;
      }

      /* Radial gradient reveal */
      .feature-card::before {
        content: '';
        position: absolute;
        top: 50%;
        left: 50%;
        width: 0;
        height: 0;
        background: radial-gradient(
          circle,
          rgba(216, 74, 140, 0.3) 0%,
          transparent 70%
        );
        transform: translate(-50%, -50%);
        transition: all 0.6s ease;
        border-radius: 50%;
      }

      .feature-card:hover::before {
        width: 500px;
        height: 500px;
      }

      .feature-card:hover {
        transform: translateY(-15px);
        box-shadow: 0 30px 70px rgba(216, 74, 140, 0.5);
        border-color: var(--orange);
      }

      .feature-icon {
        font-size: 4rem;
        margin-bottom: 25px;
        display: block;
        filter: drop-shadow(0 5px 15px rgba(255, 107, 53, 0.4));
        transition: all 0.3s ease;
      }

      .feature-card:hover .feature-icon {
        transform: scale(1.2) rotate(360deg);
      }

      .feature-title {
        font-size: 1.5rem;
        font-weight: 700;
        color: var(--gold);
        margin-bottom: 18px;
        transition: color 0.3s ease;
      }

      .feature-card:hover .feature-title {
        color: var(--orange);
      }

      .feature-desc {
        color: var(--text-light);
        line-height: 1.7;
        font-size: 1.05rem;
      }

      .feature-desc a {
        color: var(--gold);
        text-decoration: underline;
        font-weight: 700;
        transition: all 0.3s ease;
      }

      .feature-desc a:hover {
        color: var(--orange);
        text-shadow: 0 0 10px rgba(255, 107, 53, 0.5);
      }

      /* Footer */
      footer {
        text-align: center;
        padding: 70px 0 50px;
        color: var(--text-light);
        opacity: 0.8;
        border-top: 2px solid rgba(139, 58, 140, 0.3);
        margin-top: 100px;
        position: relative;
      }

      footer::before {
        content: '⭐';
        position: absolute;
        top: -20px;
        left: 50%;
        transform: translateX(-50%);
        font-size: 2rem;
        animation: spin 10s linear infinite;
      }

      @keyframes spin {
        from {
          transform: translateX(-50%) rotate(0deg);
        }
        to {
          transform: translateX(-50%) rotate(360deg);
        }
      }

      footer p {
        margin: 12px 0;
        font-size: 1.05rem;
      }

      footer a {
        color: var(--orange);
        text-decoration: none;
        transition: color 0.3s ease;
      }

      footer a:hover {
        color: var(--gold);
        text-decoration: underline;
      }

      /* Inline links in content */
      section p a {
        color: var(--orange);
        text-decoration: none;
        font-weight: 600;
        transition: all 0.3s ease;
        border-bottom: 2px solid transparent;
      }

      section p a:hover {
        color: var(--gold);
        border-bottom-color: var(--gold);
      }

      /* Tooltip */
      .tooltip {
        position: relative;
        display: inline-block;
      }

      .tooltip .tooltiptext {
        visibility: hidden;
        width: 200px;
        background-color: rgba(26, 11, 46, 0.95);
