    width: 100%;
    border: 1px solid var(--color-border-lighter);
    border-radius: 4px;
    padding: 10px;
    margin: 10px 0;
  }

  #console .console_error {
    color: var(--color-console-error);
    background-color: var(--color-console-error-bg);
    font-family: monospace;
  }

  #console .console_error:hover {
    background-color: var(--color-console-error-bg-hover);
  }

  #console .console_log {
    background: var(--color-bg-console);
    font-family: monospace;
  }

  #console .console_log:hover {
    background: var(--color-console-log-bg-hover);
  }

