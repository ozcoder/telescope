    gap: 10px 0px;
    padding: 8px;
    font-weight: 600;
    border-bottom: 2px solid var(--color-border-dark);
    font-size: 13px;
    background: var(--color-bg-light);
  }

  .network-url {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-family: monospace;
    font-size: 12px;
  }

  .network-timeline-container {
    position: relative;
    height: 20px;
    border-radius: 2px;
  }

  .network-timeline-bar {
    position: absolute;
    height: 100%;
    border-radius: 2px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 10px;
    color: white;
    font-weight: 600;
  }

