    background: var(--color-document);
  }

  .type-stylesheet {
    background: var(--color-stylesheet);
  }

  .type-script {
    background: var(--color-script);
  }

  .type-image {
    background: var(--color-image);
  }

  .type-font {
    background: var(--color-font);
  }

  .type-json {
