  }

  .legend-label {
    font-weight: 500;
  }

  .legend-timing {
    font-size: 10pt;
    color: var(--color-text-light);
  }

  .legend-color {
    width: 10px;
    height: 10px;
  }

  .dns {
    background: var(--color-dns);
  }

  .tcp,
  .connect {
    background: var(--color-tcp);
  }

  .ssl {
    background: var(--color-ssl);
  }

  .request,
  .send {
    background: var(--color-request);
  }

  .response,
  .wait {
    background: var(--color-response);
  }

  .processing,
  .receive {
    background: var(--color-processing);
  }

  .domContentLoaded {
    background: var(--color-dom-content);
  }

  .complete {
    background: var(--color-complete);
  }

