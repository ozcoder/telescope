    margin: 20px 0;
  }

  .network-request {
    display: grid;
    grid-template-columns: 25% 8% 8% 10% 49%;
    gap: 10px 0px;
    align-items: center;
    border-bottom: 1px solid var(--color-border-lighter);
    font-size: 13px;
    padding: 5px;
    border-radius: 2px;
