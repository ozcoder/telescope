    max-width: 200px;
  }

  .timing-timeline {
    background: #e9ecef;
    border-radius: 2px;
    height: 30px;
    display: flex;
    margin: 20px 0;
    overflow: hidden;
  }

  .timing-phase {
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 10pt;
    font-weight: 600;
  }
  .timing-phase:hover {
    opacity: 0.9;
